{
  "extends": "airbnb",
  "rules": {
      "indent": [2, 4],
      "max-len": [2, 200, 4],
      "func-names": [0],
      "arrow-body-style": [0],
      "no-param-reassign": [0],
      "import/no-extraneous-dependencies": [0],
      "react/jsx-filename-extension": [0],
      "react/jsx-indent-props": [2, 4],
      "react/jsx-indent": [2, 4],
      "react/prop-types": [0],
      "no-undef": [0],
      "jsx-a11y/no-static-element-interactions": [0]
  },
  "parserOptions":{
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  }
}
